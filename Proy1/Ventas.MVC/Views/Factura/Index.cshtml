@model IEnumerable<Proy1_ENT.Entities.Factura>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Concepto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaEmision)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FacturaId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ruc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Igv)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImporteTotal)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Concepto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaEmision)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FacturaId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ruc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Igv)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImporteTotal)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ComprobanteId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ComprobanteId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ComprobanteId })
        </td>
    </tr>
}

</table>
