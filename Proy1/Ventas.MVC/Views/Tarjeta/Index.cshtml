@model IEnumerable<Proy1_ENT.Entities.Tarjeta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TarjetaId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TarjetaId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Tipo_PagoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.Tipo_PagoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Tipo_PagoId })
        </td>
    </tr>
}

</table>
